{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "Logs/errorLog-.txt",
                "rollingInterval": "Day"
              }
            }
          ]
        }
      }
    ],
    "Enrich": [ "WithExceptionDetails" ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "localDb": "Data Source=database.db"
  },
  "TransactionSettings": {
    "Pattern": "\\d{19}//",
    "DefaultDebit": "0",
    "Attributes": {
      "TransactionDate": "BrojZaReklamaciju",
      "Reference": "Referenca",
      "Recipient": "NalogKorisnik",
      "TransactionDescription": "Opis",
      "Debit": "Duguje"
    }
  }
}
